package oopIntro;

//constructor

public class Product {
	public Product(){										//constructor olusturmak.
		System.out.println("Ben calistim !");
	}
	
	public Product() {										//overloading (biden fazla constructor olustruma)
		
	}
	
	int id;
	String name;
	double unitPrice;
	String detail;
}
